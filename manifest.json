{
    "manifest_version": 2,
  
    "name": "LMS Test Solver",
    "description": "Boost tests solving!",
    "version": "1.0.0",
    "icons": { "128": "solver-fav.png" },
    "browser_action": {
      "default_icon": "solver-fav.png",
      "default_popup": "extension/ext.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["tabs"]
}